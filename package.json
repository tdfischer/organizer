{
  "name": "organizer",
  "version": "0.0.1",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "heroku-postbuild": "yarn run build",
    "build": "webpack -p --config webpack.prod.js",
    "frontend": "webpack-dev-server --config webpack.dev.js",
    "backend": "python manage.py runserver",
    "worker": "python manage.py rqworker",
    "redis": "redis-server",
    "start": "concurrently -c \"bgYellow,bgBlue.bold,bgGreen.bold,bgMagenta.bold,bgRed\" -p name --names \"JEST,WEBPACK,DJANGO,WORKER,REDIS\" --kill-others npm:watch-test npm:frontend npm:backend npm:worker npm:redis",
    "watch-test": "jest --watch --color --coverage",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "@material-ui/core": "^1.4.1",
    "@turf/distance": "^6.0.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "8",
    "babel-loader": "^7.1.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "base-64": "^0.1.0",
    "bloomfilter": "^0.0.18",
    "concurrently": "^3.5.1",
    "connected-react-router": "^4.3.0",
    "css-loader": "^0.28.4",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "estraverse-fb": "^1.3.2",
    "fast-check": "^1.4.0",
    "file-loader": "^1.1.11",
    "gravatar": "^1.6.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "informed": "^1.7.2",
    "leaflet": "~1.3.1",
    "leaflet-geosearch": "^2.6.0",
    "leaflet.locatecontrol": "^0.62.0",
    "leaflet.markercluster": "^1.3.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "preprocess-loader": "^0.2.2",
    "promise-queue": "^2.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-add-to-calendar": "^0.1.5",
    "react-dom": "^16.3.2",
    "react-form": "^3.5.5",
    "react-imported-component": "^4.4.1",
    "react-leaflet": "^1.9.1",
    "react-leaflet-heatmap-layer": "^1.0.4",
    "react-leaflet-markercluster": "^1.1.8",
    "react-markdown": "^3.3.2",
    "react-places-autocomplete": "^7.1.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^7.0.1",
    "sheetclip": "^0.3.0",
    "slug": "^0.9.1",
    "style-loader": "^0.21.0",
    "svg-react-loader": "^0.4.5",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "4",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-merge": "^4.1.0",
    "workbox-webpack-plugin": "^3.3.1",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    },
    "plugins": [
      "syntax-dynamic-import",
      "transform-object-rest-spread",
      "react-hot-loader/babel"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.x",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.10.0",
    "fetch-mock": "^6.5.2",
    "jest": "^22.4.3",
    "jest-junit": "^5.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "4.2.0",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "3"
  },
  "jest-junit": {
    "output": "./test-results/jest/results.xml"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testMatch": [
      "**/*test.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "assets/js/**/*.js",
      "!**/node_modules/**"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "/node_modules/(?!react-leaflet-markercluster).+\\.js$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>assets/js/setupTests.js"
  }
}
